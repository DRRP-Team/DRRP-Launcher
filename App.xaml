<Application x:Class="DRRP_Launcher_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DRRP_Launcher_WPF"
             StartupUri="LauncherWindow.xaml">
    <Application.Resources>

        <SolidColorBrush x:Key="TextColor" Color="White"/>
        <SolidColorBrush x:Key="Overlay" Color="#3F000000"/>
        <FontFamily x:Key="Font1">Exo 2</FontFamily>
        <SolidColorBrush x:Key="TextGray" Color="#FFA4A4A4"/>
        <SolidColorBrush x:Key="TextTextBox" Color="Red"/>

    </Application.Resources>
</Application>
